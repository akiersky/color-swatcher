{
  "name": "color-swatcher",
  "version": "1.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "color-swatcher",
    "manifest": "src/manifest.json",
    "main": "color-swatcher.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiersky/color-swatcher.git"
  }
}
